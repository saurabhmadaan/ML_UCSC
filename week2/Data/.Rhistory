install.packages("RSVGTipsDevice")
library(RSVGTipsDevice)
# the data
x<-rnorm(10,0,1)
y<-x+rnorm(10,0,1)
# Create a graphics device to 'record' whatever you are plotting
devSVGTips("SVGPLOT_saurabh.svg",title="Saurabh trial.csv",toolTipMode=2)
# Just an empty plot to set the graph up
plot(-2:2,-2:2,col="white",xlab="X Co-ordinate",ylab="Y Coordinate")
# Go through each point and associate a tooltip with it
for ( i in 1:length(x)){
# set the tooltip
tooltip_string<-paste("Tooltip",i)
setSVGShapeContents(paste("<desc1>",tooltip_string,"</desc1>"))
# draw the point
points(x[i],y[i],cex=2,col="blue",bg="#0000FF80")
}
# finish writing to the device and turn it off
dev.off()
